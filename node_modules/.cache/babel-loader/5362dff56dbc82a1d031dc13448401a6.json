{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Mybutton from '@/components/Mybutton.vue';\nexport default {\n  name: \"Login\",\n  components: {\n    Mybutton\n  },\n\n  data() {\n    return {\n      username: '',\n      password: '',\n      active: 0\n    };\n  },\n\n  methods: {\n    click_re() {\n      this.$router.push({\n        name: 'Register'\n      });\n    },\n\n    click_login() {\n      this.$axios({\n        method: 'post',\n        url: '/api/signin/',\n        data: JSON.stringify({\n          userId: this.username,\n          passWord: this.password\n        })\n      }).then(res => {\n        this.$router.push({\n          path: '/main',\n          query: {\n            userId: this.username\n          }\n        });\n      }).catch(err => {\n        if (err.response) {\n          if (err.response.status == 401) {\n            alert('密码错误！');\n          } else {\n            alert('用户名不存在');\n          }\n        }\n      });\n    },\n\n    classification(a) {\n      this.active = a;\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA+DA;AACA;EACAA,aADA;EAEAC;IACAC;EADA,CAFA;;EAKAC;IACA;MACAC,YADA;MAEAC,YAFA;MAGAC;IAHA;EAKA,CAXA;;EAYAC;IACAC;MACA;QAAAR;MAAA;IACA,CAHA;;IAIAS;MACA;QACAC,cADA;QAEAC,mBAFA;QAGAR;UACAS,qBADA;UAEAC;QAFA;MAHA,GAQAC,IARA,CAQAC;QACA;UAAAC;UAAAC;YAAAL;UAAA;QAAA;MACA,CAVA,EAUAM,KAVA,CAUAC;QACA;UACA;YACAC;UACA,CAFA,MAEA;YACAA;UACA;QACA;MACA,CAlBA;IAmBA,CAxBA;;IAyBAC;MACA;IACA;;EA3BA;AAZA","names":["name","components","Mybutton","data","username","password","active","methods","click_re","click_login","method","url","userId","passWord","then","res","path","query","catch","err","alert","classification"],"sourceRoot":"src/views","sources":["Login.vue"],"sourcesContent":["<template>\n<div>\n    <Mybutton msg=\"登录\"></Mybutton>\n    <div class=\"topBox\">\n        <img src=\"../assets/logo.png\" width=\"82px\">\n        <div class=\"logo\" style=\"text-align:center;display: inline-block;\">\n            <div>烧瓶</div>\n            <div style=\"font-size: 16px;font:Microsoft Yahei;\">心仪生活上烧瓶</div>\n        </div>\n        <div style=\"display:inline-block;font-size:30px;font-weight:500;color: #606266;\">\n            欢迎登录\n        </div>\n    </div>\n\n    <div class=\"main\">\n        <div class=\"window\">\n            <div style=\"text-align: center;border-bottom: 2px solid rgb(241, 241, 241);padding-bottom: 10px;\">\n                <!--<p style=\"font-size: 30px;font-weight: 600;margin-top: 20px; padding-top: 20px;\">卖家登录</p>-->\n                <div style=\"font-size: 25px;font-weight: 500;margin-top: 20px; padding-top: 20px;\"> \n                    <div @click.prevent=\"classification(0)\" style=\"display:inline-block;width: 40%;\">\n                        <a href=\"#\" :class=\"{'activeCss':active==0}\">买家登录</a>\n                    </div>\n\n                    <div @click.prevent=\"classification(1)\" style=\"display:inline-block;width: 40%;border-left: 2px solid rgb(241, 241, 241);\">\n                        <a href=\"#\" :class=\"{'activeCss':active==1}\">卖家登录</a>\n                    </div>\n                </div>    \n            </div>\n            <form>\n                <div id=\"input1\">\n                    <input v-model=\"username\" type=\"text\" placeholder=\"账号\" class=\"inputBox\" style=\"font-size: 18px;\">\n                </div>\n\n                <div id=\"input2\">\n                    <input v-model=\"password\" type=\"password\" placeholder=\"密码\" class=\"inputBox\" style=\"font-size: 18px;\">\n                </div>\n            </form>\n            \n\n            <div style=\"margin-top: 60px;\">\n                <el-button type=\"primary\" @click=\"click_login\" style=\"width: 200px;\">登    录</el-button>\n            </div>\n            <div>\n                <a @click.prevent=\"click_re\" href=\"#\" target=\"_blank\" style=\"text-decoration: none;\">\n                    <div style=\"margin-top: 35px;background-color: rgb(239, 234, 234);padding-top: 20px;padding-bottom: 20px;border-radius:0 0 10px 10px;\">\n                        立即注册\n                    </div>\n                </a>\n            </div>\n        </div>\n    </div>\n\n    <!-- <div style=\"text-align:center;margin-top: 10px;\">\n        <a href=\"#\" style=\"text-decoration: none;\">联系我们</a>\n    </div> -->\n\n    <footer>\n        Copyright © 2022-2022  烧瓶shopping.com 版权所有\n    </footer>\n</div>\n</template>\n  \n  <script>\n  import Mybutton from '@/components/Mybutton.vue';\n  export default {\n    name: \"Login\",\n    components:{\n        Mybutton\n    },\n    data() {\n      return {\n        username: '',\n        password: '',\n        active: 0\n      }\n    },\n    methods: {\n      click_re() {\n        this.$router.push({name:'Register'});\n      },\n      click_login() {\n        this.$axios({\n          method: 'post',\n          url: '/api/signin/',\n          data: JSON.stringify({\n            userId: this.username,\n            passWord: this.password\n          })\n        })\n        .then(res => {\n            this.$router.push({path:'/main',query: {userId: this.username} });\n        }).catch ((err) => {\n            if (err.response){\n                if (err.response.status == 401) {\n                    alert('密码错误！');\n                } else {\n                    alert('用户名不存在');\n                }\n            }\n        })\n      },\n      classification(a) {\n        this.active = a;\n      }\n    }\n  }\n\n  </script>\n  \n  <style scoped>\n        .logo {\n            display: inline-block;\n            color: rgb(174, 0, 255);\n            font-size: 30px;\n            font-weight: 600;\n            margin-right: 40px;\n        }\n        .topBox {\n            padding-right: 45%;\n            padding-top: 9px;\n            padding-bottom: 18px;\n            font:\"Microsoft YaHei\";\n        }\n        .main {\n            width: 100%;\n            height: 520px;\n            background-color: rgba(247, 154, 211, 0.483);\n            background-image: url(../assets/loginbg.jpg);\n            background-size: 100%;\n            background-attachment: fixed;\n            margin: 0px;\n            padding-top: 40px;\n\n        }\n        a:hover {\n            color: red;\n        }   \n        a {\n            text-decoration: none;\n        }\n        .window {\n            width: 330px;\n            height: 400px;\n            background-color: white;\n            margin-left: 65%;\n            text-align: center;\n            padding-bottom: 0%;\n            border-radius: 4%;\n        }\n        .fool {\n            font-size: 10px;\n            font-family: STSong;\n            text-align: center;\n            background-color: rgb(247, 241, 233);\n            padding-top: 10px;\n            padding-bottom: 10px;\n        }\n        #input1 {\n            width: 300px;\n            text-align: center;\n            background-image: url(../assets/login1.jpg);\n            background-repeat: no-repeat;\n            background-position: left;\n            border-width: 1px;\n            border-style: solid;\n            border-color: gray;\n            margin-top: 30px;\n            margin-left: 15px;\n            padding-left: 8px;\n        }\n        #input2 {\n            width: 300px;\n            text-align: center;\n            background-image: url(../assets/login2.jpg);\n            background-repeat: no-repeat;\n            background-position: left;\n            border-width: 1px;\n            border-style: solid;\n            border-color: gray;\n            margin-top: 30px;\n            margin-left: 15px;\n            padding-left: 8px;\n        }\n        .inputBox {\n            outline: 0px;\n            border: 0px;\n            height: 40px;\n        }\n        button:hover{\n            background-color: cornflowerblue;\n        }\n        button{\n            color: white;\n            background-color: rgb(192, 48, 221);\n            font-size: 20px;\n        }\n\n    footer {\n        background: black;\n        color: gray;\n        font-size: 13px;\n        padding: 20px 20px;\n        text-align: center;\n        font-weight: bold;\n        font-family: 'Courier New', Courier, monospace;\n    }\n    .activeCss {\n        color: red;\n    }\n  </style>"]},"metadata":{},"sourceType":"module"}