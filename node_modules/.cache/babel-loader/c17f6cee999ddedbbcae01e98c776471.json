{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Mybutton from '@/components/Mybutton.vue';\nexport default {\n  name: \"Login\",\n  components: {\n    Mybutton\n  },\n\n  data() {\n    return {\n      username: '',\n      password: '',\n      active: 0\n    };\n  },\n\n  methods: {\n    click_re() {\n      this.$router.push({\n        name: 'Register'\n      });\n    },\n\n    click_login() {\n      alert('111');\n      this.$axios({\n        method: 'post',\n        url: '/api/signin/',\n        data: JSON.stringify({\n          userId: this.username,\n          passWord: this.password\n        })\n      }).then(res => {\n        this.$router.push({\n          path: '/main',\n          query: {\n            userId: this.username\n          }\n        });\n      }).catch(err => {\n        if (err.response) {\n          if (err.response.status == 401) {\n            alert('密码错误！');\n          } else {\n            alert('用户名不存在');\n          }\n        }\n      });\n    },\n\n    classification(a) {\n      this.active = a;\n    }\n\n  }\n};","map":{"version":3,"mappings":";AA0CA;AACA;EACAA,aADA;EAEAC;IACAC;EADA,CAFA;;EAKAC;IACA;MACAC,YADA;MAEAC,YAFA;MAGAC;IAHA;EAKA,CAXA;;EAYAC;IACAC;MACA;QAAAR;MAAA;IACA,CAHA;;IAIAS;MACAC;MACA;QACAC,cADA;QAEAC,mBAFA;QAGAT;UACAU,qBADA;UAEAC;QAFA;MAHA,GAQAC,IARA,CAQAC;QACA;UAAAC;UAAAC;YAAAL;UAAA;QAAA;MACA,CAVA,EAUAM,KAVA,CAUAC;QACA;UACA;YACAV;UACA,CAFA,MAEA;YACAA;UACA;QACA;MACA,CAlBA;IAmBA,CAzBA;;IA0BAW;MACA;IACA;;EA5BA;AAZA","names":["name","components","Mybutton","data","username","password","active","methods","click_re","click_login","alert","method","url","userId","passWord","then","res","path","query","catch","err","classification"],"sourceRoot":"src/views","sources":["Login.vue"],"sourcesContent":["<template>\n<div>\n    <div class=\"topBox\">\n        <img src=\"../assets/logo.png\" width=\"82px\">\n        <div class=\"logo\" style=\"text-align:center;display: inline-block;\">\n            <div>烧瓶</div>\